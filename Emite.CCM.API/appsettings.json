{
  "Application": "Emite.CCM API",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": "xxx"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Properties": {
      "Application": "Emite.CCM.Api"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Issuer": "https://localhost:5001/",
    "Audience": "https://localhost:44379"
  },
  "ConnectionStrings": {
    "ApplicationContext": "Server=(localdb)\\mssqllocaldb;Database=CCM;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "UseInMemoryDatabase": false,
  "Version": {
    "ReleaseName": "1.0.0.0",
    "BuildNumber": "19000101.0"
  }
}
