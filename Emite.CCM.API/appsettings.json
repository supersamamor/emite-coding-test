{
  "Application": "Emite.CCM API",
  "CacheSettings": {
    "DefaultCacheDurationMinutes": 5
  },
  "HttpClientSettings": {
    "AllowInvalidCertificates": true
  },
  "RateLimiter": {
    "NumberOfRequest": 100,
    "WindowTimeInMinutes": 1
  },
  "Elasticsearch": {
    "Uri": ""
  },
  "SslCertificate": {
    "Path": "",
    "Password": ""
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": "xxx"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Properties": {
      "Application": "Emite.CCM.Api"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Issuer": "https://localhost:48021",
    "Audience": "https://localhost:48023"
  },
  "ConnectionStrings": {
    "ApplicationContext": "Server=localhost;Database=CCM;Trusted_Connection=True;MultipleActiveResultSets=true;TrustServerCertificate=True"
  },
  "UseInMemoryDatabase": false,
  "Version": {
    "ReleaseName": "1.0.0.0",
    "BuildNumber": "19000101.0"
  }
}
